logical opeartor
	&&(and)  ----> comprision
	              if (a>=0 && a<=0)

	    true && true =t
	    T && f= f
	    f && t=f
	    f && f= f
	
	|| -->  t || t =t
		f || t=t
		T || f =t
		f || f=f
	if(a>0 || a<=0 && a!=5)
	
	!(not)  -->  
	!false= true
	!true= false
